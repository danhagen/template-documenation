@article{Kesar2009,
   abstract = {BACKGROUND AND PURPOSE: Functional electrical stimulation (FES) is
a popular poststroke gait rehabilitation intervention. Although stroke
causes multijoint gait deficits, FES is commonly used only for the
correction of swing-phase foot drop. Ankle plantarflexor muscles
play an important role during gait. The aim of the current study
was to test the immediate effects of delivering FES to both ankle
plantarflexors and dorsiflexors on poststroke gait. METHODS: Gait
analysis was performed as subjects (N=13) with chronic poststroke
hemiparesis walked at their self-selected walking speeds during walking
with and without FES. RESULTS: Compared with delivering FES to only
the ankle dorsiflexor muscles during the swing phase, delivering
FES to both the paretic ankle plantarflexors during terminal stance
and dorsiflexors during the swing phase provided the advantage of
greater swing-phase knee flexion, greater ankle plantarflexion angle
at toe-off, and greater forward propulsion. Although FES of both
the dorsiflexor and plantarflexor muscles improved swing-phase ankle
dorsiflexion compared with noFES, the improvement was less than that
observed by stimulating the dorsiflexors alone, suggesting the need
to further optimize stimulation parameters and timing for the dorsiflexor
muscles during gait. CONCLUSIONS: In contrast to the typical FES
approach of stimulating ankle dorsiflexor muscles only during the
swing phase, delivering FES to both the plantarflexor and dorsiflexor
muscles can help to correct poststroke gait deficits at multiple
joints (ankle and knee) during both the swing and stance phases of
gait. Our study shows the feasibility and advantages of stimulating
the ankle plantarflexors during FES for poststroke gait.},
   author = {Trisha M Kesar and Ramu Perumal and Darcy S Reisman and Angela Jancosko and Katherine S Rudolph and Jill S Higginson and Stuart A Binder-Macleod},
   issue = {12},
   institution = {Department of Physical Therapy, University of Delaware, Newark, DE 19702, USA. kesar@udel.edu},
   journal = {Stroke},
   keywords = {Aged; Ankle Joint,Articular,Neurologic,Skeletal,methods; Female; Gait Disorders,physiology,physiology; Electric Stimulation Therapy,physiology; Humans; Knee,physiology; Male; Middle Aged; Muscle,physiology; Paresis,physiology; Recovery of Function,physiology; Stroke,physiopathology/therapy; Range of Motion,therapy; Gait,therapy; Treatment Outcome; Walking},
   month = {12},
   pages = {3821-3827},
   pmid = {19834018},
   title = {Functional electrical stimulation of ankle plantarflexor and dorsiflexor muscles: effects on poststroke gait.},
   volume = {40},
   year = {2009},
}
