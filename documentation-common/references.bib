   issue = {6},
   institution = {University of Nantes, UFR STAPS, Laboratoire Motricit�, Interactions, Performance, 44322 Nantes cedex 3, France.},
   journal = {J Appl Physiol},
   keywords = {Adult; Elasticity Imaging Techniques,Skeletal,instrumentation/methods; Electric Stimulation,methods; Electromyography; Humans; Male; Muscle C,physiology/ultrasonography,physiology/ultrasonography; Reaction Time,physiology; Muscle,physiology; Tendons},
   month = {6},
   pages = {1970-1975},
   pmid = {19359617},
   title = {Electromechanical delay revisited using very high frame rate ultrasound.},
   volume = {106},
   url = {http://dx.doi.org/10.1152/japplphysiol.00221.2009},
   year = {2009},
}
