@article{Giangregorio2006,
   abstract = {Individuals with spinal cord injury (SCI) often experience bone loss and muscle atrophy. Muscle atrophy can result in reduced metabolic rate and increase the risk of metabolic disorders. Sublesional osteoporosis predisposes individuals with SCI to an increased risk of low-trauma fracture. Fractures in people with SCI have been reported during transfers from bed to chair, and while being turned in bed. The bone loss and muscle atrophy that occur after SCI are substantial and may be influenced by factors such as completeness of injury or time postinjury. A number of interventions, including standing, electrically stimulated cycling or resistance training, and walking exercises have been explored with the aim of reducing bone loss and/or increasing bone mass and muscle mass in individuals with SCI. Exercise with electrical stimulation appears to increase muscle mass and/or prevent atrophy, but studies investigating its effect on bone are conflicting. Several methodological limitations in exercise studies with individuals with SCI to date limit our ability to confirm the utility of exercise for improving skeletal status. The impact of standing or walking exercises on muscle and bone has not been well established. Future research should carefully consider the study design, skeletal measurement sites, and the measurement techniques used in order to facilitate sound conclusions.},
   author = {Lora Giangregorio and Neil McCartney},
   isbn = {1079-0268 (Print)\r1079-0268 (Linking)},
   issn = {1079-0268},
   issue = {5},
   journal = {The journal of spinal cord medicine},
   keywords = {Electric Stimulation Therapy,Exercise,Exercise: physiology,Fractures, Bone,Fractures, Bone: etiology,Fractures, Bone: prevention & control,Humans,Muscular Atrophy,Muscular Atrophy: etiology,Muscular Atrophy: prevention & control,Muscular Atrophy: rehabilitation,Osteoporosis,Osteoporosis: etiology,Osteoporosis: prevention & control,Osteoporosis: rehabilitation,Predictive Value of Tests,Prevalence,Risk Factors,Spinal Cord Injuries,Spinal Cord Injuries: complications,Spinal Cord Injuries: physiopathology,Spinal Cord Injuries: rehabilitation,Walking,Walking: physiology},
   pages = {489-500},
   pmid = {17274487},
   title = {Bone loss and muscle atrophy in spinal cord injury: epidemiology, fracture prediction, and rehabilitation strategies.},
   volume = {29},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1949032&tool=pmcentrez&rendertype=abstract},
   year = {2006},
}
@article{Kirsch2017,
   abstract = {Minimizing the amount of electrical stimulation can potentially mitigate the adverse effects of muscle fatigue during functional electrical stimulation (FES) induced limb movements. A gradient projection-based model predictive controller is presented for optimal control of a knee extension elicited via FES. A control Lyapunov function was used as a terminal cost to ensure stability of the model predictive control. The controller validation results show that the algorithm can be implemented in real-time with a steady-state RMS error of less than 2°. The experiments also show that the controller follows step changes in desired angles and is robust to external disturbances.},
