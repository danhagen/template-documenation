   journal = {Electromyogr. Clin. Neurophysiol.},
   keywords = {Adult; Ankle,Physical; Weight-Bearing,Skeletal,physiology,physiology; Braces; Electromyography; Female; Hum,physiology; Muscle,physiology; Muscle Contraction,physiology; Posture,physiology; Reaction Time,physiology; Restraint},
   month = {11},
   pages = {349-354},
   pmid = {17147077},
   title = {The effects of external support on electromechanical delay of the peroneus longus muscle},
   volume = {46},
   year = {2006},
}
