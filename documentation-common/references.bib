   issn = {1872-678X},
   issue = {1},
   journal = {Journal of Neuroscience Methods},
   keywords = {Adult,Aged,Electric Stimulation,Electric Stimulation: instrumentation,Electric Stimulation: methods,Exercise Therapy,Exercise Therapy: methods,Female,Hemiplegia,Hemiplegia: pathology,Hemiplegia: rehabilitation,Humans,Leg,Leg: physiopathology,Male,Middle Aged,Questionnaires,Treatment Outcome,Walking,Walking: physiology},
   month = {6},
   pages = {100-105},
   pmid = {19383515},
   title = {Sensor-driven four-channel stimulation of paretic leg: functional electrical walking therapy},
   volume = {181},
   url = {http://www.ncbi.nlm.nih.gov/pubmed/19383515},
   year = {2009},
}
@article{Karu1995,
   abstract = {Applications of electrical stimulation for restoration of functional
movements such as standing, gait, and grasp have always been hindered
by the rapid fatigue of stimulated muscle. This paper describes an
experimental investigation of stimulation with N-lets (a set of N
closely spaced stimulation pulses) as a means of producing contractions
with improved fatigue characteristics. Experiments were conducted
on 27 able-bodied and four SCI human subjects using surface stimulation
of the quadriceps muscle to produce isometric knee joint torque.
Based upon evidence from the literature on muscle fatigue, parameters
of the N-let trains for N = 1-6 were optimized to produce the most
force per pulse. The results demonstrated that: 1) nonlinear summation
of the twitch response occurs in human subjects with N-let surface
stimulation; 2) for most subjects, doublet stimulation (N = 2) with
a pulse interval of about 5 ms produced the maximum torque-time integral
per pulse of the resulting twitch; and 3) on average, optimal N-let
stimulation resulted in a 36% increase in isometric torque tracking
when compared to traditional singlet stimulation. The results have
immediate implications for alleviating the problem of premature fatigue
during functional electrical stimulation.},
   author = {Z Z Karu and W K Durfee and a M Barzilai},
   doi = {10.1109/10.398642},
   issn = {0018-9294},
   issue = {8},
   institution = {Department of Electrical Engineering, Massachusetts Institute of Technology, Cambridge 02139, USA.},
   journal = {IEEE Trans. Biomed. Eng.},
   keywords = {Adult,Electric Stimulation,Female,Humans,Male,Muscle Fatigue,Muscle Fatigue: physiology,physiology},
   month = {8},
   pages = {809-817},
   pmid = {7642195},
   title = {Reducing muscle fatigue in FES applications by stimulating with N-let pulse trains},
   volume = {42},
   url = {http://dx.doi.org/10.1109/10.398642 http://www.ncbi.nlm.nih.gov/pubmed/7642195},
   year = {1995},
}
