@article{Chien2002,
   abstract = {Control of integrating processes with long deadtime is a challenging
problem. Using original Smith predictor control structure will result
in an offset problem during load disturbance. Using the internal
model control (IMC) principle to design the control structure will
result in large overshoot during servo response. In this work, a
simple modified Smith predictor controller design is proposed for
this important type of system. The overall control structure has
only two physically meaningful tuning parameters. One is used to
set the speed of the closed-loop servo response and the other tuning
parameter is used to set the speed of the closed-loop load response.
Two examples are used to demonstrate the improved closed-loop performance
of the proposed controller design. (C) 2002 Elsevier Science Ltd.
All rights reserved.},
   author = {I L Chien and S C Peng and J H Liu},
   city = {THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND},
   issn = {0959-1524},
   issue = {3},
   journal = {J. Process Control},
   keywords = {deadtime compensation; Smith predictor; integratin},
   pages = {391-404},
