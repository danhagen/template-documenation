@article{Thrasher2005,
   abstract = {A major limitation of many functional electrical stimulation (FES)
applications is that muscles tend to fatigue very rapidly. It was
hypothesized that FES-induced muscle fatigue could be reduced by
randomly modulating the pulse frequency, amplitude, and pulse width
in a range of +/-15%. Seven subjects with spinal-cord injuries participated
in this study. FES was applied to quadriceps and tibialis anterior
muscles using surface electrodes. Isometric force was measured, and
the time for the force to drop by 3 dB (fatigue time) was compared
between trials. Four different modes of FES were applied in random
order: constant stimulation, randomized frequency, randomized amplitude,
and randomized pulse width. There was no significant difference between
the fatigue-time measurements for the four modes of stimulation (P=0.329).
Therefore, random modulation appeared to have no effect. Based on
an observed correlation between maximum force measurements and trial
order, we concluded that having 10-min rest periods between trials
was insufficient.},
   author = {A Thrasher and G M Graham and M R Popovic},
   doi = {10.1111/j.1525-1594.2005.29076.x},
   issue = {6},
   institution = {Institute of Biomaterials and Biomedical Engineering, University of Toronto, Canada. adam.thrasher@utoronto.ca},
   journal = {Artif. Organs},
   keywords = {Adult; Analysis of Variance; Electric Stimulation,Skeletal,methods; Female; Humans; Isometric Contraction,physiology; Leg; Male; Muscle Fatigue,physiology; Muscle,physiopathology/rehabilitation,physiopathology; Spinal Cord Injuries},
   month = {6},
   pages = {453-458},
   pmid = {15926981},
   title = {Reducing muscle fatigue due to functional electrical stimulation using random modulation of stimulation parameters},
   volume = {29},
   url = {http://dx.doi.org/10.1111/j.1525-1594.2005.29076.x},
   year = {2005},
}
