@article{Planitzer2014,
   abstract = {INTRODUCTION: Functional electrical stimulation (FES) serves as a possible therapy to restore missing motor functions of peripheral nerves by means of cuff electrodes. FES is established for improving lower limb function. Transferring this method to the upper extremity is complex, due to a lack of anatomical data on the physiological configuration of nerve fascicles. Our study's aim was to provide an anatomical basis for FES of the median nerve in the distal forearm and hand.

METHODS: We investigated 21 distal median nerves from 12 body donors. The peripheral fascicles were traced back by removing the external and interfascicular epineurium and then assigned to 4 quadrants.

RESULTS: A distinct motor and sensory distribution was observed. The fascicles innervating the thenar eminence and the first lumbrical muscle originated from the nerves' radial parts in 82%. The fascicle supplying the second lumbrical muscle originated from the ulnar side in 78%. No macroscopically visible plexus formation was observed for the distal median nerve in the forearm.

CONCLUSIONS: The findings on the distribution of the motor branches of the median nerve and the missing plexus formation may likely serve as an anatomical basis for FES of the distal forearm. However, due to the considerable variability of the motor branches, cuff electrodes will need to be adapted individually in FES. Taking into account the sensory distribution of the median nerve, FES may also possibly be applied in the treatment of regional pain syndromes.},
   author = {Uwe Planitzer and Hanno Steinke and Jürgen Meixensberger and Ingo Bechmann and Niels Hammer and Dirk Winkler},
   doi = {10.1016/j.aanat.2013.11.002},
   issn = {1618-0402},
   issue = {2-3},
   journal = {Annals of anatomy = Anatomischer Anzeiger : official organ of the Anatomische Gesellschaft},
   keywords = {Anatomy,Electrical stimulation,FES,FM,FR,MN,MTE,Median nerve fascicles,PCB,Prosthesis,Rehabilitation,S,SPA,TE,d,dorsal,flexor muscles of the forearm,flexor retinaculum,functional electrical stimulation,median nerve,motor fascicle to the thenar eminence,muscles of the thenar eminence,n,p,palmar,palmar cutaneous branch,r,radial,sample size,skin,superficial palmar arch,u,ulnar},
   month = {5},
   pages = {144-9},
   pmid = {24374103},
   title = {Median nerve fascicular anatomy as a basis for distal neural prostheses.},
   volume = {196},
   url = {http://www.sciencedirect.com/science/article/pii/S0940960213001660},
   year = {2014},
}
